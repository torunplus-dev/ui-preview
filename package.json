{
  "name": "ui-preview-workspace",
  "private": true,
  "version": "0.1.0",
  "engines": {
    "node": "22.x"
  },
  "packageManager": "pnpm@9.12.0",
  "scripts": {
    "dev": "pnpm --filter ui-preview dev",
    "build": "pnpm --filter ui-preview build && rm -rf dist && cp -R apps/ui-preview/dist dist",
    "preview": "pnpm --filter ui-preview preview",
    "typecheck": "pnpm --filter ui-preview typecheck"
  }
}
